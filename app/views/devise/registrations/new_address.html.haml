%h2 住所情報登録
= form_for @address do |f|
  = render "devise/shared/error_messages", resource: @address
  .field
    = f.label :address_family_name
    %br/
    = f.text_field :address_family_name
  .field
    = f.label :address_first_name
    %br/
    = f.text_field :address_first_name
  .field
    = f.label :address_family_kana_name
    %br/
    = f.text_field :address_family_kana_name
  .field
    = f.label :address_first_kana_name
    %br/
    = f.text_field :address_first_kana_name
  .field
    = f.label :zipcode
    %br/
    = f.text_field :zipcode
  .field
    = f.label :prefecture
    %br/
    = f.text_field :prefecture
  .field
    = f.label :city
    %br/
    = f.text_field :city
  .field
    = f.label :block
    %br/
    = f.text_field :block
  .field
    = f.label :building
    %br/
    = f.text_field :building
  .field
    = f.label :tel
    %br/
    = f.text_field :tel
  .actions
    = f.submit "Next"
= render "devise/shared/links"