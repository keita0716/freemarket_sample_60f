<h2>住所情報登録</h2>

<%= form_for @address do |f| %>
  <%= render "devise/shared/error_messages", resource: @address %>


<div class="field">
    <%= f.label :address_family_name %><br />
    <%= f.text_field :address_family_name  %>
  </div>

<div class="field">
    <%= f.label :address_first_name %><br />
    <%= f.text_field :address_first_name  %>
  </div>

<div class="field">
    <%= f.label :address_family_kana_name %><br />
    <%= f.text_field :address_family_kana_name  %>
  </div>

<div class="field">
    <%= f.label :address_first_kana_name%><br />
    <%= f.text_field :address_first_kana_name  %>
  </div>

  <div class="field">
    <%= f.label :zipcode %><br />
    <%= f.text_field :zipcode %>
  </div>

  <div class="field">
    <%= f.label :prefecture %><br />
    <%= f.text_field :prefecture %>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>


 <div class="field">
    <%= f.label :block %><br />
    <%= f.text_field :block %>
  </div>

   <div class="field">
    <%= f.label :building %><br />
    <%= f.text_field :building %>
  </div>

   <div class="field">
    <%= f.label :tel %><br />
    <%= f.text_field :tel %>
  </div>

 <div class="actions">
    <%= f.submit "Next" %>
  </div>
<% end %>


<%= render "devise/shared/links" %>